        }

        .blue {
            color: #4a5d75;
        }

        .green {
            color: #4a5d75;
        }

        a {
            color: #4a5d75;
        }

            /* NAVIGATION */

        #nav li {
            list-style-type: none;
        }

        #nav ul {
            padding-left: 15px;
        }

        #nav>ul {
            padding-left: 5px;
        }

        #nav>ul>li {
            color: #000;
            padding: 3px 0;
            margin: 0 0 10px 0;
        }

        #nav ul li ul li {
            padding: 3px 2px;
        }

        li.active {
            background-color: #C6CCD4;
            font-weight: bold;
            color: #000;
        }

            /* TABLES */
        table tr td, table tr th {
            border: 1px solid #4a5d75;
            border-collapse: collapse;
            padding: 2px;
            border-left: none;
            border-right: none;
        }

        table {
            border-collapse: collapse;
            margin: 15px 0;
            border: 1px solid #4a5d75;
        }

        table tr.odd {
            background-color: #FFF;
        }

        table tr.even {
            background-color: #e2e5e9;
        }

        table th {
            background-color: #c6ccd4;
            text-align: left;
            color: #000;
        }

            /* HEADINGS */

        h1 {
            font-size: 18px;
            font-weight: bold;
            padding-bottom: 12px;
        }

        h2 {
            font-size: 16px;
            font-weight: bold;
            padding-bottom: 10px;
        }
    </style>
