            margin: 0 10px;
        }

        #header {
            width: 100%;
            margin: 0 0 25px 0;
            border-bottom: 1px solid #4a5d75;
            height: 55px;
            background-image: url(/_ah/admin/res/capedwarf_logo_200px.gif);
            background-repeat: no-repeat;
        }

        #sidebar {
            float: left;
            clear: left;
            width: 170px;
            border-right: 2px solid #C6CCD4;
            color: #4a5d75
        }

        #content {
            width: auto;
            float: none;
            margin: 0 0 0 170px;
            padding-left: 15px;
        }

        p {
